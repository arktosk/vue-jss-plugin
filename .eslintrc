{
  "root": true,
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "google"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "ecmaFeatures": {
    "classes": true
  },
  "ignorePatterns": ["build/", "examples/"],
  "rules": {
    "max-len": "off",
    "linebreak-style": "off"
  }
}
