{
  "name": "vue-jss-plugin",
  "version": "0.0.0-alpha.4",
  "description": "JSS implementation for Vue.js",
  "main": "build/vue-jss-plugin.min.js",
  "files": [
    "src/**/*",
    "build/vue-jss-plugin.min.js"
