{
  "name": "vue-jss-project",
  "version": "1.1.0-alpha.0",
  "description": "JSS implementation project for Vue.js",
  "private": true,
  "scripts": {
    "clean": "lerna run clean && rimraf node_modules",
    "bootstrap": "lerna bootstrap",
    "start": "lerna run start --stream",
    "start:plugin": "lerna run start --scope=vue-jss-plugin --stream",
    "lint": "lerna run lint --stream",
    "lint:plugin": "lerna run lint --scope=vue-jss-plugin --stream",
    "test": "lerna run test --stream",
    "test:plugin": "lerna run test --scope=vue-jss-plugin --stream",
    "build": "lerna run build --stream",
    "build:plugin": "lerna run build --scope=vue-jss-plugin --stream",
    "publish:next": "lerna publish --yes --npm-tag next",
    "publish:latest": "lerna publish --yes"
  },
  "author": {
    "name": "Arkadiusz S. Krauzowicz",
    "email": "arkadiusz.krauzowicz@cube11.pl",
    "url": "https://cube11.pl/"
  },
  "license": "MIT",
  "homepage": "https://github.com/arktosk/vue-jss-project",
  "repository": {
    "type": "git",
    "url": "https://github.com/arktosk/vue-jss-project.git"
  },
  "devDependencies": {
    "lerna": "^3.16.4",
    "rimraf": "^3.0.0"
  }
}
