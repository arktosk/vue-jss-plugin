{
  "name": "vue-jss-plugin",
  "version": "1.2.1-next.2",
  "description": "JSS implementation plugin for Vue.js",
  "keywords": [
    "css-in-js",
    "jss",
    "styles",
    "vue",
    "vue-jss"
  ],
  "homepage": "https://github.com/arktosk/vue-jss-plugin/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/arktosk/vue-jss-plugin/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arktosk/vue-jss-plugin.git",
    "directory": "packages/vue-jss-plugin"
  },
  "license": "MIT",
  "author": {
    "name": "Arkadiusz S. Krauzowicz",
    "email": "arkadiusz.krauzowicz@cube11.pl",
    "url": "https://cube11.pl/"
  },
  "files": [
    "src/**/*.js",
    "!src/**/*.test.js",
    "build/**"
  ],
  "main": "build/vue-jss-plugin.umd.js",
  "module": "src/index.js",
  "scripts": {
    "build": "webpack --config=webpack.config.js",
    "clean": "rimraf build",
