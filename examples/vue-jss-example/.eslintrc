{
  "root": true,
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": [
      "google",
      "plugin:vue/recommended"
  ],
  "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "plugins": [
      "vue",
      "html"
  ],
  "rules": {
      "max-len": "off",
      "linebreak-style": "off"
  }
}